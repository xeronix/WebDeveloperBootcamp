<h1>Results Page !!!</h1>

<ul>
    <% data["Search"].forEach(function(movie) { %>
        <li> 
            <a href="/movie/<%=movie["Title"]%>"><strong>
                <%= movie["Title"] %>
            </a></strong>
            - <%= movie["Year"] %>
        </li>
    <%    });%>
</ul>

<a href="/" >Search Again!</a>